template(name="home")
  div.container
    div.row
      h1 gitrt
      h2 files
      +files

template(name="files")
  ul
    each files
      li
        a(href="{{pathFor 'fileEdit' path}}") {{path}}

template(name="fileEdit")
  div.container
    div.row
      h1 gitrt
      a.commit(href='#') save project
      h2 files
      +files
      h3 {{fileInfo._id}}
      textarea#editor

